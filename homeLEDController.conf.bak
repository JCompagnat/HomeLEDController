

WSGIPythonHome "/usr/local/bin"
WSGIPythonPath "/var/www/venv/lib/python3.4/site-packages"

<VirtualHost *>
 #ServerName tero.example.com

 WSGIDaemonProcess homeLEDController user=pi group=www-data threads=5
 WSGIScriptAlias / /var/www/homeLEDController/homeLEDController.wsgi 
 
<Directory /var/www/homeLEDController/>
 WSGIProcessGroup homeLEDController
 WSGIApplicationGroup %{GLOBAL}
 WSGIScriptReloading On
 Require all granted
</Directory>
</VirtualHost>
